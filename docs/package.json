    "@rails/webpacker": "4.2.2",
    "@kajabi/sage": "file:../javascript",
    "core-js": "^3.6.5",
    "arrive": "^2.4.1",
    "left-pad": "^1.3.0",
    "sortablejs": "^1.10.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
