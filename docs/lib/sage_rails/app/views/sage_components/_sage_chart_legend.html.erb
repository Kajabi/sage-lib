<ul
  class="sage-chart-legend  <%= component.generated_css_classes %>"
  <%= component.generated_html_attributes.html_safe %>
>
  <% component.items.each do | item | %>
    <li
      class="sage-chart-legend__item"
      style="--item-color: <%= item[:color] %>"
    >
      <%= item[:title] %>
    </li>
  <% end %>
</ul>
