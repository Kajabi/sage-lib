{{#each components}}

//
// {{name}}
//
export {{name}} = {
  {{!-- First look in props for any with options and output union types and token --}}
  tokens: {
    {{#each props}}
      {{#if options}}
    {{name}}: {
        {{#each options}}
      "{{name}}": {{{value}}}
        {{/each}}
      {{/if}}
    },
    {{/each}}
  },
  {{!-- Next look in props for any defaultProps to set up --}}
  defaults: {
    {{#each props}}
      {{name}}: {{{default.value}}},
    {{/each}}
  },
  {{!-- Last, output the actual PropType declaration --}}
  props: {
    {{#each props}}
      {{#if comment}}
    // {{{comment}}}
      {{/if}}
    {{name}}: {{type.value}},
    {{/each}}
  },
};
{{/each}}
